                    email:' '\n                }\n            }\n        },\n        components:{\n            TextBox\n        },\n        methods: {\n            sendDetails() {\n                this.action='new';\n            },\n            validateRegNo(reg_no) {\n    \n                if(reg_no.length!==4) return false;\n                for(let i=0;i<reg_no.length;i++) {\n                    if(reg_no[i]<='0' || reg_no[i]>='9') {\n                        return false;\n                    }\n                }\n                return true;\n            }\n            ,\n            see() {\n                console.log(this.student);\n\n                if(!this.validateRegNo(this.student.id)) {this.error_message='Invalid Register Number';this.show_error=true;setTimeout(()=>{this.show_error=false;},3000);}\n                else {\n               axios.post('http://localhost:8068/student/add',this.student).then((res)=>{\n                   if(res.data.status==='success') {\n                       this.success=true;\n                   }\n                   else {alert('User Already exists');}\n               })\n                }\n            }\n        }\n}\n</script>\n\n<style>\ninput {\n    font-size:20px;\n  width: 40%;\n  padding: 12px 20px;\n  margin: 8px 0;\n  box-sizing: border-box;\n  border: 2px solid rgb(146, 16, 169);\n  border-radius: 4px;\n}\nlabel {\n  font-size:20px;\n  font-weight:bold;\n  width: 40%;\n  padding: 12px 20px;\n  margin: 8px 0;\n  box-sizing: border-box;\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}