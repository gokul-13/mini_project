{"ast":null,"code":"import TextBox from '../components/TextBox.vue';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      action: 'old',\n      error_message: '',\n      success: false,\n      show_error: false,\n      student: {\n        id: '',\n        name: '',\n        email: ' '\n      }\n    };\n  },\n\n  components: {\n    TextBox\n  },\n  methods: {\n    sendDetails() {\n      this.action = 'new';\n    },\n\n    validateRegNo(reg_no) {\n      if (reg_no.length !== 4) return false;\n\n      for (let i = 0; i < reg_no.length; i++) {\n        if (reg_no[i] <= '0' || reg_no[i] >= '9') {\n          return false;\n        }\n      }\n\n      return true;\n    },\n\n    see() {\n      console.log(this.student);\n\n      if (!this.validateRegNo(this.student.id)) {\n        this.error_message = 'Invalid Register Number';\n        this.show_error = true;\n        setTimeout(() => {\n          this.show_error = false;\n        }, 3000);\n      } else {\n        axios.post('http://localhost:8068/student/add', this.student).then(res => {\n          if (res.data.status === 'success') {\n            this.success = true;\n          } else {\n            alert('User Already exists');\n          }\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,OAAP,MAAoB,2BAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AACPC,MAAI,GAAG;AACH,WAAO;AACHC,YAAM,EAAC,KADJ;AAEHC,mBAAa,EAAC,EAFX;AAGHC,aAAO,EAAC,KAHL;AAIHC,gBAAU,EAAC,KAJR;AAKHC,aAAO,EAAE;AACLC,UAAE,EAAC,EADE;AAELC,YAAI,EAAC,EAFA;AAGLC,aAAK,EAAC;AAHD;AALN,KAAP;AAWH,GAbM;;AAcPC,YAAU,EAAC;AACPX;AADO,GAdJ;AAiBPY,SAAO,EAAE;AACLC,eAAW,GAAG;AACV,WAAKV,MAAL,GAAY,KAAZ;AACH,KAHI;;AAILW,iBAAa,CAACC,MAAD,EAAS;AAElB,UAAGA,MAAM,CAACC,MAAP,KAAgB,CAAnB,EAAsB,OAAO,KAAP;;AACtB,WAAI,IAAIC,CAAC,GAAC,CAAV,EAAYA,CAAC,GAACF,MAAM,CAACC,MAArB,EAA4BC,CAAC,EAA7B,EAAiC;AAC7B,YAAGF,MAAM,CAACE,CAAD,CAAN,IAAW,GAAX,IAAkBF,MAAM,CAACE,CAAD,CAAN,IAAW,GAAhC,EAAqC;AACjC,iBAAO,KAAP;AACJ;AACJ;;AACA,aAAO,IAAP;AACJ,K