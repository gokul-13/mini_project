{"ast":null,"code":"import axios from 'axios';\n\nconst validate = require('../validation');\n\nexport default {\n  data() {\n    return {\n      name_show: false,\n      cname_show: false,\n      name: '',\n      cname: '',\n      reg_no: '',\n      c_no: '',\n      mark: '',\n      success: false,\n      failue: false,\n      error_message: \"\"\n    };\n  },\n\n  methods: {\n    async getName() {\n      //if(!validate.validateRegNo(this.reg_no,4)) {this.error_message='Invalid Register No';alert(this.error_message);return;}\n      axios.get(`http://localhost:8068/student/detail/${this.reg_no}`).then(res => {\n        console.log(res.data);\n\n        if (res.data.length == 0) {\n          alert(\"Invalid Register Number\");\n          this.reg_no = '';\n          this.name = '';\n          return;\n        }\n\n        this.name = res.data[0].name;\n        this.name_show = true;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    async getCourse() {\n      if (!validate.validateRegNo(this.c_no, 3)) {\n        this.error_message = 'Invalid Course Id';\n        alert(this.error_message);\n        return;\n      }\n\n      axios.get(`http://localhost:8068/course/detail/${this.c_no}`).then(res => {\n        console.log(res.data);\n\n        if (res.data.length == 0) {\n          alert(\"Invalid Course Number\");\n          this.c_no = '';\n          this.cname = '';\n          return;\n        }\n\n        this.cname = res.data[0].name;\n        this.cname_show = true;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    async submitMarks() {\n      axios.post('http://localhost:8068/mark/add', {\n        sid: this.reg_no,\n        mark: this.mark,\n        cid: this.c_no\n      }).then(res => {\n        if (res.data.status === 'success') {\n          console.log(res);\n          this.success = true;\n          setTimeout(() => {\n            this.success = false;\n            this.$router.go();\n          }, 2000);\n        } else {\n          this.failue = true;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkBE,OAAOA,KAAP,MAAkB,OAAlB;;AACA,MAAMC,QAAO,GAAIC,OAAO,CAAC,eAAD,CAAxB;;AAEF,eAAe;AAEXC,MAAI,GAAG;AACH,WAAO;AACHC,eAAS,EAAC,KADP;AAEHC,gBAAU,EAAC,KAFR;AAGHC,UAAI,EAAC,EAHF;AAIHC,WAAK,EAAC,EAJH;AAKHC,YAAM,EAAC,EALJ;AAMHC,UAAI,EAAC,EANF;AAOHC,UAAI,EAAC,EAPF;AAQHC,aAAO,EAAC,KARL;AASHC,YAAM,EAAC,KATJ;AAUHC,mBAAa,EAAC;AAVX,KAAP;AAYH,GAfU;;AAgBXC,SAAO,EAAE;AACL,UAAMC,OAAN,GAAgB;AACZ;AAEAf,WAAK,CAACgB,GAAN,CAAW,wCAAuC,KAAKR,MAAO,EAA9D,EAAiES,IAAjE,CAAuEC,GAAD,IAAO;AACzEC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACf,IAAhB;;AACA,YAAGe,GAAG,CAACf,IAAJ,CAASkB,MAAT,IAAiB,CAApB,EAAuB;AAACC,eAAK,CAAC,yBAAD,CAAL;AAAiC,eAAKd,MAAL,GAAY,EAAZ;AAAe,eAAKF,IAAL,GAAU,EAAV;AAAa;AAAO;;AAC5F,aAAKA,IAAL,GAAUY,GAAG,CAACf,IAAJ,CAAS,CAAT,EAAYG,IAAtB;AACA,aAAKF,SAAL,GAAe,IAAf;AACH,OALD,EAKGmB,KALH,CAKUC,GAAD,IAAO;AACZL,eAAO,CAACC,GAAR,CAAYI,GAAZ;AACH,OAPD;AAQH,KAZI;;AAaL,UAAMC,SAAN,GAAkB;AACb,UAAG,CAACxB,QAAQ,CAACyB,aAAT,CAAuB,KAAKjB,IAA5B,EAAiC,CAAjC,CAAJ,EAAyC;AAAC,aAAKI,aAAL,GAAmB,mBAAnB;AAAuCS,aAAK,CAAC,KAAKT,aAAN,CAAL;AAA0B;AAAO;;AAEnHb,WAAK,CAACgB,GAAN,CAAW,uCAAsC,KAAKP,IAAK,EAA3D,EAA8DQ,IAA9D,CAAoEC,GAAD,IAAO;AACtEC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACf,IAAhB;;AACA,YAAGe,GAAG,CAACf,IAAJ,CAASkB,MAAT,IAAiB,CAApB,EAAuB;AAACC,eAAK,CAAC,uBAAD,CAAL;AAA+B,eAAKb,IAAL,GAAU,EAAV;AAAa,eAAKF,KAAL,GAAW,EAAX;AAAc;AAAO;;AACzF,aAAKA,KAAL,GAAWW,GAAG,CAACf,IAAJ,CAAS,CAAT,EAAYG,IAAvB;AACA,aAAKD,UAAL,GAAgB,IAAhB;AACH,OALD,EAKGkB,KALH,CAKUC,GAAD,IAAO;AACZL,eAAO,CAACC,GAAR,CAAYI,GAAZ;AACH,OAPD;AAQH,KAxBI;;AAyBL,UAAMG,WAAN,GAAoB;AAChB3B,WAAK,CAAC4B,IAAN,CAAW,gCAAX,EAA4C;AAACC,WAAG,EAAC,KAAKrB,MAAV;AAAiBE,YAAI,EAAC,KAAKA,IAA3B;AAAgCoB,WAAG,EAAC,KAAKrB;AAAzC,OAA5C,EAA4FQ,IAA5F,CAAkGC,GAAD,IAAO;AACpG,YAAGA,GAAG,CAACf,IAAJ,CAAS4B,MAAT,KAAkB,SAArB,EAAgC;AAC5BZ,iBAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,eAAKP,OAAL,GAAa,IAAb;AACAqB,oBAAU,CAAC,MAAI;AAAC,iBAAKrB,OAAL,GAAa,KAAb;AAAmB,iBAAKsB,OAAL,CAAaC,EAAb;AAAkB,WAA3C,EAA4C,IAA5C,CAAV;AACJ,SAJA,MAKK;AACD,eAAKtB,MAAL,GAAY,IAAZ;AACJ;AACH,OATD;AAUJ;;AApCK;AAhBE,CAAf","names":["axios","validate","require","data","name_show","cname_show","name","cname","reg_no","c_no","mark","success","failue","error_message","methods","getName","get","then","res","console","log","length","alert","catch","err","getCourse","validateRegNo","submitMarks","post","sid","cid","status","setTimeout","$router","go"],"sourceRoot":"","sources":["/Users/ecom-gokula.vp/Documents/Samsung_docs/mini_project/client/src/views/AddMarks.vue"],"sourcesContent":["<template>\n   <h2 v-show='success' style=\"color:green\"> Marks Inserted Sucessfully</h2>\n   <h2 v-show='failue' style=\"color:red\"> Oops!!Marks Inserted Failed!!</h2>\n    <label class='form__label'>Registration No</label><br>\n   <input type='number' placeholder=\"Enter the student Registration Number\" v-model='reg_no' @blur='getName' required><br>\n    <label class='form__label' v-show=\"name_show\">Student Name</label><br>\n   <input type='name' placeholder=\"course_name\" v-show='name_show' :value='name' disabled><br>\n    <label class='form__label'>Course id</label><br>\n   <input type='number' placeholder=\"Enter the Course No\" v-model='c_no' @blur='getCourse' required><br>\n    <label class='form__label' v-show=\"cname_show\">Course Name</label><br>\n   <input type='text'  v-show=\"cname_show\" :value='cname' disabled><br>\n    <label class='form__label'>Mark Scored</label><br>\n   <input type='text' placeholder=\"Enter the Marks\" v-model=\"mark\"><br>\n   <button @click='submitMarks'> Click to Submit</button>\n   \n</template>\n\n<script>\n  import axios from 'axios';\n  const validate = require('../validation');\n\nexport default {\n  \n    data() {\n        return {\n            name_show:false,\n            cname_show:false,\n            name:'',\n            cname:'',\n            reg_no:'',\n            c_no:'',\n            mark:'',\n            success:false,\n            failue:false,\n            error_message:\"\"\n        }\n    },\n    methods: {\n        async getName() {\n            //if(!validate.validateRegNo(this.reg_no,4)) {this.error_message='Invalid Register No';alert(this.error_message);return;}\n\n            axios.get(`http://localhost:8068/student/detail/${this.reg_no}`).then((res)=>{\n                console.log(res.data);\n                if(res.data.length==0) {alert(\"Invalid Register Number\");this.reg_no='';this.name='';return;}\n                this.name=res.data[0].name;\n                this.name_show=true;\n            }).catch((err)=>{\n                console.log(err);\n            })\n        },\n        async getCourse() {\n             if(!validate.validateRegNo(this.c_no,3)) {this.error_message='Invalid Course Id';alert(this.error_message);return;}\n\n            axios.get(`http://localhost:8068/course/detail/${this.c_no}`).then((res)=>{\n                console.log(res.data);\n                if(res.data.length==0) {alert(\"Invalid Course Number\");this.c_no='';this.cname='';return;}\n                this.cname=res.data[0].name;\n                this.cname_show=true;\n            }).catch((err)=>{\n                console.log(err);\n            })\n        },\n        async submitMarks() {\n            axios.post('http://localhost:8068/mark/add',{sid:this.reg_no,mark:this.mark,cid:this.c_no}).then((res)=>{\n                if(res.data.status==='success') {\n                    console.log(res);\n                    this.success=true;\n                    setTimeout(()=>{this.success=false;this.$router.go()},2000);\n                }\n                else {\n                    this.failue=true;\n                }\n            })\n        }\n    }\n\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}