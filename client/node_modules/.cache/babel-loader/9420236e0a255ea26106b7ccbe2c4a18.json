{"ast":null,"code":"import TextBox from '../components/TextBox.vue';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      action: 'old',\n      error_message: '',\n      success: false,\n      show_error: false,\n      student: {\n        id: '',\n        name: '',\n        email: ''\n      }\n    };\n  },\n\n  components: {\n    TextBox\n  },\n  methods: {\n    sendDetails() {\n      this.action = 'new';\n    },\n\n    validateRegNo(reg_no) {\n      console.log(\"called:\" + reg_no.length);\n      if (reg_no.length !== 5) return false;\n\n      for (let i = 1; i < reg_no.length; i++) {\n        console.log(reg_no[i]);\n\n        if (reg_no[i] < '0' || reg_no[i] > '9') {\n          return false;\n        }\n      }\n\n      return true;\n    },\n\n    see() {\n      console.log(this.student);\n\n      if (!this.validateRegNo(this.student.id)) {\n        this.error_message = 'Invalid Register Number';\n        alert(this.error_message);\n        return;\n      } else {\n        axios.post('http://localhost:8068/student/add', this.student).then(res => {\n          if (res.data.status === 'success') {\n            this.success = true;\n          } else {\n            alert('User Already exists');\n          }\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,OAAP,MAAoB,2BAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AACPC,MAAI,GAAG;AACH,WAAO;AACHC,YAAM,EAAC,KADJ;AAEHC,mBAAa,EAAC,EAFX;AAGHC,aAAO,EAAC,KAHL;AAIHC,gBAAU,EAAC,KAJR;AAKHC,aAAO,EAAE;AACLC,UAAE,EAAC,EADE;AAELC,YAAI,EAAC,EAFA;AAGLC,aAAK,EAAC;AAHD;AALN,KAAP;AAWH,GAbM;;AAcPC,YAAU,EAAC;AACPX;AADO,GAdJ;AAiBPY,SAAO,EAAE;AACLC,eAAW,GAAG;AACV,WAAKV,MAAL,GAAY,KAAZ;AACH,KAHI;;AAILW,iBAAa,CAACC,MAAD,EAAS;AAClBC,aAAO,CAACC,GAAR,CAAY,YAAUF,MAAM,CAACG,MAA7B;AACA,UAAGH,MAAM,CAACG,MAAP,KAAgB,CAAnB,EAAsB,OAAO,KAAP;;AACtB,WAAI,IAAIC,CAAC,GAAC,CAAV,EAAYA,CAAC,GAACJ,MAAM,CAACG,MAArB,EAA4BC,CAAC,EAA7B,EAAiC;AAC7BH,eAAO,CAACC,GAAR,CAAYF,MAAM,CAACI,CAAD,CAAlB;;AACA,YAAGJ,MAAM,CA