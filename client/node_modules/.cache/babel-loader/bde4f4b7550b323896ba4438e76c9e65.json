{"ast":null,"code":"import TextBox from '../components/TextBox.vue';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      action: 'old',\n      error_message: '',\n      success: false,\n      show_error: false,\n      student: {\n        id: '',\n        name: '',\n        email: ''\n      }\n    };\n  },\n\n  components: {\n    TextBox\n  },\n  methods: {\n    sendDetails() {\n      this.action = 'new';\n    },\n\n    validateRegNo(reg_no) {\n      console.log(\"called:\" + reg_no.length);\n      if (reg_no.length !== 4) return false;\n\n      for (let i = 0; i < reg_no.length; i++) {\n        console.log(reg_no[i]);\n\n        if (reg_no[i] < '0' || reg_no[i] > '9') {\n          return false;\n        }\n      }\n\n      return true;\n    },\n\n    validateName(name) {\n      console.log(\"called:\" + name.length);\n\n      for (let i = 0; i < name.length; i++) {\n        console.log(name[i]);\n\n        if (name[i] >= 'a' && name[i] <= 'z' || name[i] >= 'A' && name[i] <= 'Z') {} else return false;\n      }\n\n      return true;\n    },\n\n    see() {\n      console.log(this.student);\n\n      if (!this.validateRegNo(this.student.id)) {\n        this.error_message = 'Invalid Register Number';\n        alert(this.error_message);\n        return;\n      }\n\n      if (!this.validateName(this.student.name)) {\n        this.error_message = 'Invalid Student Name';\n        alert(this.error_message);\n        return;\n      } else {\n        axios.post('http://localhost:8068/student/add', this.student).then(res => {\n          if (res.data.status === 'success') {\n            this.success = true;\n            setTimeout(() => {\n              this.$router.go();\n            }, 2000);\n          } else {\n            alert('User Already exists');\n          }\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,OAAP,MAAoB,2BAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AACPC,MAAI,GAAG;AACH,WAAO;AACHC,YAAM,EAAC,KADJ;AA