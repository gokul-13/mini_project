{"ast":null,"code":"import axios from 'axios';\nimport router from '../router/index';\nexport default {\n  data() {\n    return {\n      courses: []\n    };\n  },\n\n  name: 'HomeView',\n  components: {},\n  methods: {\n    addStudent() {\n      console.log(\"Called\");\n      this.$router.push({\n        name: 'addStudent'\n      });\n    },\n\n    getList() {\n      console.log('Student List called');\n      router.push({\n        name: 'getlist'\n      });\n    },\n\n    addCourse() {\n      this.$router.push({\n        name: 'addcourse'\n      });\n    }\n\n  },\n\n  created() {\n    axios.get('http://localhost:8086/course/all').then(res => {\n      this.courses = res.data;\n      console.log(res);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAiCC,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACD,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAC;AADH,KAAP;AAGD,GALY;;AAMbC,MAAI,EAAE,UANO;AAObC,YAAU,EAAE,EAPC;AAQbC,SAAO,EAAE;AACLC,cAAU,GAAG;AACTC,aAAO,CAACC,GAAR,CAAY,QAAZ;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAACP,YAAI,EAAC;AAAN,OAAlB;AAEH,KALI;;AAMJQ,WAAO,GAAG;AACTJ,aAAO,CAACC,GAAR,CAAY,qBAAZ;AACER,YAAM,CAACU,IAAP,CAAY;AAACP,YAAI,EAAC;AAAN,OAAZ;AAEH,KAVI;;AAWLS,aAAS,GAAG;AACV,WAAKH,OAAL,CAAaC,IAAb,CAAkB;AAACP,YAAI,EAAC;AAAN,OAAlB;AACF;;AAbK,GARI;;AAwBbU,SAAO,GAAG;AACNd,SAAK,CAACe,GAAN,CAAU,kCAAV,EAA8CC,IAA9C,CAAoDC,GAAD,IAAO;AACxD,WAAKd,OAAL,GAAac,GAAG,CAACf,IAAjB;AACAM,aAAO,CAACC,GAAR,CAAYQ,GAAZ;AACD,KAHD;AAIJ;;AA7Ba,CAAf","names":["axios","router","data","courses","name","components","methods","addStudent","console","log","$router","push","getList","addCourse","created","get","then","res"],"sourceRoot":"","sources":["/Users/ecom-gokula.vp/Documents/Samsung_docs/mini_project/client/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div >\n    <h1>Welcome!!  What do you want to do??</h1>\n    <button @click='addStudent'>Add a student</button>\n    <button @click='getList'>Get the Students List</button>\n    <button @click='addCourse'>Add a Course</button>\n    <button @click='$router.push(\"addmarks\");'>Add Marks</button>\n    <button @click='$router.push(\"viewstudent\");'>View Student</button>\n\n    <div>\n       <h2>Courses We Offer</h2>\n       <table id=\"student\">\n\n         <tr>\n           <th> Course Id</th>\n           <th> Course Name</th>\n       </tr>\n       <tr v-for='record in courses' :key=\"record.name\">\n           <td v-for='(field) in record' :key='field.name'>\n              {{field}}\n           </td>\n       </tr>\n\n\n\n\n       </table>\n       \n    </div>\n  </div>\n</template>\n\n<script>\n import axios from 'axios';\n import router from '../router/index'\nexport default {\n  data() {\n    return {\n      courses:[]\n    }\n  },\n  name: 'HomeView',\n  components: {},\n  methods: {\n      addStudent() {\n          console.log(\"Called\");\n          this.$router.push({name:'addStudent'});\n          \n      },\n       getList() {\n        console.log('Student List called');\n          router.push({name:'getlist'});\n       \n      },\n      addCourse() {\n        this.$router.push({name:'addcourse'});\n      }\n\n  },\n  created() {\n      axios.get('http://localhost:8086/course/all').then((res)=>{\n        this.courses=res.data;\n        console.log(res);\n      })\n  }\n}\n</script>\n\n<style>\n\nbutton {\n \n  background-color: #af65eb; /* Green */\n  border: none;\n  color: white;\n  margin:20px;\n  padding: 15px 32px;\n  text-align: center;\n  text-decoration: none;\n  display: inline-block;\n  font-size: 16px;\n\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}