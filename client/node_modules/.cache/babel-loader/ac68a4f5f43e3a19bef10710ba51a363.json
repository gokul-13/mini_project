{"ast":null,"code":"import TextBox from '../components/TextBox';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      action: 'old',\n      error_message: '',\n      course: {\n        cid: '',\n        cname: ''\n      }\n    };\n  },\n\n  components: {\n    TextBox\n  },\n  methods: {\n    changeState() {\n      this.action = 'new';\n    },\n\n    validateRegNo(reg_no) {\n      console.log(\"called:\" + reg_no.length);\n      if (reg_no.length !== 3) return false;\n\n      for (let i = 0; i < reg_no.length; i++) {\n        console.log(reg_no[i]);\n\n        if (reg_no[i] < '0' || reg_no[i] > '9') {\n          return false;\n        }\n      }\n\n      return true;\n    },\n\n    validateName(name) {\n      console.log(\"called:\" + name.length);\n\n      for (let i = 0; i < name.length; i++) {\n        console.log(name[i]);\n\n        if (name[i] >= 'a' && name[i] <= 'z' || name[i] >= 'A' && name[i] <= 'Z') {} else return false;\n      }\n\n      return true;\n    },\n\n    async sendData() {\n      console.log('In send data');\n\n      if (!this.validateRegNo(this.course.cid)) {\n        this.error_message = 'Invalid Course Id';\n        alert(this.error_message);\n        return;\n      }\n\n      if (!this.validateName(this.course.cname)) {\n        this.error_message = 'Invalid Course Name';\n        alert(this.error_message);\n        return;\n      }\n\n      const res = await axios.post('http://localhost:8068/course/add', this.course);\n      console.log(res.data);\n      if (res.data.status === 'success') alert(\"Course Successfully added\");else {\n        alert(\"Add Failed\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,OAAP,MAAoB,uBAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;AACXC,MAAI,GAAG;AACH,WAAO;AACHC,YAAM,EAAC,KADJ;AAEHC,mBAAa,EAAC,EAFX;AAGHC,YAAM,EAAC;AACHC,WAAG,EAAC,EADD;AAEHC,aAAK,EAAC;AAFH;AAHJ,KAAP;AAQH,GAVU;;AAWXC,YAAU,EAAC;AACPR;AADO,GAXA;AAcXS,SAAO,EAAE;AACLC,eAAW,GAAG;AACN,WAAKP,MAAL,GAA